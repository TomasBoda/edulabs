-icon.svg\";\n\nexport default function Box(props) {\n    const item = props.item;\n    const icon = props.icon;\n    const delay = props.delay;\n\n    const [render, setRender] = useState(false);\n\n    setTimeout(() => {\n        setRender(true)\n    }, delay || 0);\n\n    if (!render) {\n        return null;\n    }\n\n    return(\n        <div className={\"box \" + (props.withAnimation === true ? \"animate__animated animate__zoomIn\" : \"\")} onClick={props.onClick}>\n            {props.withRemove === true ? <ion-icon name=\"close-outline\" onClick={props.onRemoveClick}></ion-icon> : null}\n\n            <img className=\"image\" src={icon ? icon : Icon} />\n            <div className=\"name\">{item.name}</div>\n        </div>\n    )\n}","export default __webpack_public_path__ + \"static/media/classroom-icon.39a5b2ea.svg\";","export default __webpack_public_path__ + \"static/media/students-icon.3eecbeac.svg\";","export default __webpack_public_path__ + \"static/media/subject-icon.7b85a9ab.svg\";","export default __webpack_public_path__ + \"static/media/class-icon.a3e9b4d2.svg\";","import React from \"react\";\nimport { Link, withRouter } from \"react-router-dom\";\n\nimport { setStorageItem, getStorageItem, removeStorageItem, isLogged } from \"../config/Config\";\n\nimport Api from \"../config/Api\";\nimport Loading from \"../components/Loading\";\nimport Panel from \"../components/Panel\";\nimport Heading from \"../components/Heading\";\nimport Edit from \"../components/Edit\";\nimport Box from \"../components/Box\";\n\nimport \"../styles/admin.css\";\n\nimport Icon from \"../assets/login-icon.svg\";\nimport ClassroomIcon from \"../assets/classroom-icon.svg\";\nimport StudentsIcon from \"../assets/students-icon.svg\";\nimport SubjectIcon from \"../assets/subject-icon.svg\";\nimport ClassIcon from \"../assets/class-icon.svg\";\n\nclass Admin extends React.Component {\n\n    state = {\n        edit: false,\n        type: \"add-classroom\"\n    }\n\n    constructor(